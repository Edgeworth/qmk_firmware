{
    "keyboard_name": "memekbd",
    "url": "",
    "bootloader": "caterina",
    "processor": "atmega32u4",
    "build": {"lto": true},
    "maintainer": "Edgeworth",
    "manufacturer": "Edgeworth",
    "features": {
        "bootmagic": false,
        "console"  : false,  // Console for debug
        "command"  : false,  // Commands for debug and configuration
        "extrakey" : true ,  // Audio control and System control
        "mousekey" : true ,  // Mouse keys
        "nkro"     : true ,  // USB Nkey Rollover
        "oled"     : false,
        "asdfr"    : false   // test to see if this breaks
    },
    "tapping": {"term": 180, "permissive_hold": true},
    "debounce": 8,
    "usb": {"device_version": "0.0.2", "vid": "0xFEED", "pid": "0x0EDC", "polling_interval": 1},
    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "rows": ["F4", "F5", "F6", "F7", "B1"],
        "cols": ["D4", "C6", "D7", "E6", "B4"]
    },
    "split": {
        "enabled": true,
        "usb_detect": {
            "enabled": true // Detect which side is the main side via USB.
        },
        "transport": {
            "protocol": "i2c",
            "watchdog": true,  // Useful to reboot on switching hubs.
            "sync": {
                "modifiers"   : true,  // Send key data to other side
                "matrix_state": true   // Send key data to other side
            }
        }
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 0},
                {"matrix": [0, 1], "x": 1, "y": 0},
                {"matrix": [0, 2], "x": 2, "y": 0},
                {"matrix": [0, 3], "x": 3, "y": 0},
                {"matrix": [0, 4], "x": 4, "y": 0},

                {"matrix": [5, 0], "x": 0, "y": 0},
                {"matrix": [5, 1], "x": 1, "y": 0},
                {"matrix": [5, 2], "x": 2, "y": 0},
                {"matrix": [5, 3], "x": 3, "y": 0},
                {"matrix": [5, 4], "x": 4, "y": 0},

                {"matrix": [1, 0], "x": 0, "y": 1},
                {"matrix": [1, 1], "x": 1, "y": 1},
                {"matrix": [1, 2], "x": 2, "y": 1},
                {"matrix": [1, 3], "x": 3, "y": 1},
                {"matrix": [1, 4], "x": 4, "y": 1},

                {"matrix": [6, 0], "x": 0, "y": 1},
                {"matrix": [6, 1], "x": 1, "y": 1},
                {"matrix": [6, 2], "x": 2, "y": 1},
                {"matrix": [6, 3], "x": 3, "y": 1},
                {"matrix": [6, 4], "x": 4, "y": 1},

                {"matrix": [2, 0], "x": 0, "y": 2},
                {"matrix": [2, 1], "x": 1, "y": 2},
                {"matrix": [2, 2], "x": 2, "y": 2},
                {"matrix": [2, 3], "x": 3, "y": 2},
                {"matrix": [2, 4], "x": 4, "y": 2},

                {"matrix": [7, 0], "x": 0, "y": 2},
                {"matrix": [7, 1], "x": 1, "y": 2},
                {"matrix": [7, 2], "x": 2, "y": 2},
                {"matrix": [7, 3], "x": 3, "y": 2},
                {"matrix": [7, 4], "x": 4, "y": 2},

                {"matrix": [3, 0], "x": 0, "y": 3},
                {"matrix": [3, 1], "x": 1, "y": 3},
                {"matrix": [3, 2], "x": 2, "y": 3},
                {"matrix": [3, 3], "x": 3, "y": 3},
                {"matrix": [3, 4], "x": 4, "y": 3},

                {"matrix": [8, 0], "x": 0, "y": 3},
